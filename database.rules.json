{
  "rules": {
    "exercises": {
      "$uid": {
        ".write": "$uid === auth.uid",
        ".read": "$uid === auth.uid",
        "$exercise": {
          "name": {".validate": "true"},
          "youtubeUrl": {".validate": "true"},
          "notes": {".validate": "true"},
          "$other": {".validate": "false"}
        }
      }
    },
    "workouts": {
      "$uid": {
        ".write": "$uid === auth.uid",
        ".read": "$uid === auth.uid",
        "$workout": {
          "name": {".validate": "true"},
          "lastAccessed": {".validate": "true"},
          "workoutSets": {
            "$workoutSet": {
              "exerciseId": {".validate": "true"},
              "targetReps": {".validate": "true"},
              "restInterval": {".validate": "true"},
              "notes": {".validate": "true"},
              "$other": {".validate": "false"} 
            }
          },
          "$other": {".validate": "false"}
        }
      }
    },
    "workoutLogs": {
      "$uid": {
        ".write": "$uid === auth.uid",
        ".read": "$uid === auth.uid",
        "$workoutLog": {
          ".indexOn": "workoutId",
          "start": {".validate": "true"},
          "end": {".validate": "true"},
          "workoutId": {".validate": "true"},
          "workoutSets": {
            "$workoutSetLog": {
              "setsCompleted": {".validate": "true"},
              "skipped": {".validate": "true"},
              "subsets": {
                "$subsetLog": {
                  "reps": {".validate": "true"},
                  "weight": {".validate": "true"},
                  "other": {".validate": "false"}
                }
              },
              "$other": {".validate": "false"}
            }
          },
          "$other": {".validate": "false"}
        }
      }
    }
  }
}
